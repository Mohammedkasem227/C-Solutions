#include <iostream>
using namespace std;

int main() {

	int A , B , C , D , X;
	cin >> A >> B >> C >> D ;
	X = (A*B)-(C*D);
	cout << "Difference  = " << X;
	
	// cin>> X; //we just use it to show the result of program cause sometimes the terminal always close

	return 0;
	
}
